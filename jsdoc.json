{
  "source": {
    "include": ["./client/src", "./server", "./README.md"],
    "includePattern": ".js$",
    "excludePattern": "node_modules"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "destination": "docs/",
    "recurse": true,
    "readme": "README.md",
    "verbose": true
  }
}
